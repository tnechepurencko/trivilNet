// Настройка:
//  тип Ключ = <тип>
//  тип Значение = <тип>
//  фн хеш(к: Ключ): Цел64 {}
//  конст размер = 1021
модуль словарь

тип Элемент = класс {
    ключ: Ключ = позже
    значение: Значение  = позже
}

тип Элементы = []мб Элемент

тип Словарь* = класс {
    элементы = Элементы[длина: размер, *: пусто] 
    кол-во: Цел64 := 0
}

фн (с: Словарь) размер*(): Цел64 {
    вернуть с.кол-во
}

тип Ключ = Строка
тип Значение = Строка
фн хеш(к: Ключ): Цел64 {}
конст размер = 1021
