модуль файлы

тип Байты* = []Байт

тип Фишка* = класс {
    путь: Строка = позже
    ошибка: мб Ошибка = пусто
}

// Выдает разделитель имен в пути
фн разделитель*(): Строка {
    вернуть "/"
}


// Выдает фишку, поставленную на рабочую папку
фн рабочая папка*(): Фишка {
    вернуть Фишка{путь: "."}
}

фн (ф: Фишка) ошибка?*(): мб Ошибка {
    вернуть ф.ошибка
}

// Добавляет к пути новые части, заданные строками
фн (ф: Фишка) добавить*(строки: ...Строка) {
    
}

