модуль юникод

// Временная упрощенная реализация
фн Буква?*(с: Символ): Лог {
    // кириллица
    если (с >= 'а' & с <= 'я') | (с >= 'А' & с <= 'Я') | с = 'ё' | с = 'Ё' { вернуть истина }
    // латиница
    если (с >= 'a' & с <= 'z') | (с >= 'A' & с <= 'Z')  { вернуть истина }
    вернуть ложь
}

фн Цифра?*(с: Символ): Лог {
    вернуть с >= '0' & с <= '9'
}

