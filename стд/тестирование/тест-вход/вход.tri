модуль тест-вход

импорт "№путь-тестируемого№"
импорт "стд::вывод"
импорт "стд::тестирование/тестер"

вход {
    вывод.ф("Тестирование...\n")

    пусть т = тестер.Тестер{}
    
    №имя-тестируемого№.Тестировать(т)
    
    пусть ош = т.число ошибочных тестов()
    если ош = 0 {
        вывод.ф("Успешно, всего тестов: %v \n", т.число успешных тестов())
    } иначе {
        вывод.ф("Ошибочных тестов %v из %v\n", т.число ошибочных тестов(), т.число тестов())
    }
}

    