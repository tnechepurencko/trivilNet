модуль x

импорт "стд::вывод"

фн Ф(а: ...*) {

    пусть д = длина(а)
    вывод.цел64(д) 
    вывод.кс()

    пусть № := 0
    пока № < д {
        вывод.слово64(нечто(а[№])) 
        вывод.строка("  ")
        вывод.слово64(тег(а[№])) 
        вывод.кс()
       
        №++
    }    
}

вход { 
  Ф(7, "A"(:Символ), ложь, истина, "строка")
}


