модуль ини

импорт "стд::вывод"

тип К = класс {
    номер: Цел64 = позже
    другой := 153
}

фн создать(н: Цел64): К {
    // проверить 'н'
    вернуть К{номер: н, другой: н * 2}
}

вход {
    пусть к := К{номер: 1}
    вывод.ф("$; $;\n", к.номер, к.другой)
    
    к := создать(2)
    вывод.ф("$; $;\n", к.номер, к.другой)
}


