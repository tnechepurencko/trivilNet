модуль оп-цикл

импорт "стд::вывод"

тип Строки = []Строка

вход {
    пусть стороны = Строки["север", "восток", "юг", "запад"]

    цикл с среди стороны {
        вывод.ф("$; ", с)
    }
    вывод.кс()

    цикл [№]с среди стороны {
        вывод.ф("$;: $;\n", №, с)
    }

    цикл [№] среди стороны {
        вывод.ф("$; ", №)
    }
    вывод.кс()


}
